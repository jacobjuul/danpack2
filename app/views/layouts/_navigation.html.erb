<header id="header">
                    <div class="container">

                        <div class="row header">

                            <!-- Logo -->
                            <div class="col-xs-2 logo">
                                <%= link_to root_path, data: { no_turbolink: true } do %>
                                    <img src="images/main_logo.png" alt="Kanzi HTML5 Template" style="height: auto;"/>
                                <% end %>
                            </div>
                            <!-- //Logo// -->


                            <!-- Navigation File -->
                            <div class="col-md-10">

                                <!-- Mobile Button Menu -->
                                <div class="mobile-menu-button">
                                    <i class="fa fa-list-ul"></i>
                                </div>
                                <!-- //Mobile Button Menu// -->




                                <nav>
                                    <ul class="navigation">
                                        
                                        <li>
                                            <%= link_to om_path, data: { no_turbolink: true } do %>
                                                <span class="label-nav">
                                                    Om Os
                                                </span>
                                                <span class="label-nav-sub" data-hover="Kompetencer">
                                                    Kompetencer
                                                </span>
                                            <% end %>
                                            
                                        </li>
                                        <li>

                                        <%= link_to kontakt_path, data: { no_turbolink: true }  do %>
                                            
                                                <span class="label-nav">
                                                    Kontakt
                                                </span>
                                                <span class="label-nav-sub" data-hover="spørgsmål">
                                                    Spørgsmål
                                                </span>
                                            <% end %>
                                            
                                        </li>
                                        
                                        
                                    </ul>

                                </nav>



 <!-- Mobile Nav. Container -->
                                <ul class="mobile-nav">
                                    <li class="responsive-searchbox">
                                        <!-- Responsive Nave -->
                                        <form action="#" method="get">
                                            <input type="text" class="searchbox-inputtext" id="searchbox-inputtext-mobile" name="s" />
                                            <button class="icon-search"></button>
                                        </form>
                                        <!-- //Responsive Nave// -->
                                    </li>
                                </ul>
                                <!-- //Mobile Nav. Container// -->
                            </div>
                            <!-- Nav -->

                        </div>



                    </div>
                </header>
                <!-- //Header// -->